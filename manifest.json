{
  "manifest_version": 2,
  "name": "Infinity Pools",
  "version": "1.2",

  "description": "An extension to hide various feeds like Facebook's, Pocket, etc. ",
  "update_url": "https://khamidou.com/extensions/update.xml",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*", "*://*.getpocket.com/*",
                  "*://*.twitter.com/*", "*://*.reddit.com/*",
                  "*://news.ycombinator.com/*"],
      "js": ["start.js"]
    }
  ]
}
